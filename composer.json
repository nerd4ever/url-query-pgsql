{
  "name": "nerd4ever/url-query-pgsql",
  "description": "A library to used to process UrlQuerySQL in PostgreSQL",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Nerd4ever Desenvolvimento Tecnologico e Inovacao Ltda",
      "email": "suporte@nerd4ever.com.br"
    }
  ],
  "keywords": [
    "php",
    "sort",
    "filter",
    "rest",
    "pgsql"
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.3.11",
    "ext-pgsql": "*",
    "nerd4ever/url-query": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.20"
  },
  "autoload": {
    "psr-4": {
      "Nerd4ever\\UrlQuery\\Sql\\UrlQueryPgSQL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nerd4ever\\UrlQuery\\Sql\\UrlQueryPgSQL\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "nerd4ever/url-query-pgsql": "*"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
